<script type="application/ld+json" nonce=<%= request.content_security_policy_nonce %>>
{
	"@context": "https://schema.org",
	"@type": "BreadcrumbList",
	"itemListElement": [{
		"@type": "ListItem",
		"position": 1,
		"name": "Accueil",
		"item": "<%= root_url %>"
	}]
}
</script>

<% content_for :meta_title, "Agence marketing web Aix en provence – SEO – site internet" %>
<% content_for :meta_description, "Créez / Optimiser votre site internet, son référencement | Dataweb Univers : agence de marketing web, SEO / SEA et développement à aix en provence – Théo Dumez" %>
<% content_for :meta_robots, "index" %>

<%= render "shared/modal-corona" %>

<%= render "shared/navbar" %>
<%= render "shared/banner" %>

<div class="nos-leviers flex-center flex-column">
	<div class="leviers flex-center flex-row mt-4">
		<%= image_tag(asset_path('fond1.png'), class: 'fond-levier1') %>
		<div class="levier flex-center flex-column index1">
			<div class="cercle">
				<%= image_tag(asset_path('computer.svg'), alt: 'Création de site web', class: 'card_img') %>
			</div>
			<p>Création de site web</p>
		</div>
		<div class="levier flex-center flex-column index1">
			<div class="cercle">
				<%= image_tag(asset_path('search.svg'), alt: 'Référencement (SEO/SEA)', class: 'card_img') %>
			</div>
			<p>Référencement (SEO/SEA)</p>
		</div>
		<div class="levier flex-center flex-column index1">
			<div class="cercle">
				<%= image_tag(asset_path('analytics.svg'), alt: 'Expérience Utilisateur (UX)', class: 'card_img') %>
			</div>
			<p>Expérience Utilisateur (UX)</p>
		</div>
		<%= image_tag(asset_path('fond3.png'), class: 'fond-levier3') %>
	</div>
</div>

<div class="agence flex-center flex-row">
	<div class="agence-img background-img w40-h483 index1" data-aos="fade-up-right"></div>
	<div class="flex-center w50-1" data-aos="zoom-in-left">
		<div class="agence-text index1">
			<h2 class="black my-5 h2 rb-110">DataWeb univers est une agence de marketing digital et de référencement</h2>
			<p class="black my-5 h2 rb-55">
				Notre entreprise est fière de générer du trafic sur votre site et de convertir vos visiteurs en clients.
			</p>
			<p class="black my-5 h2">
				Contactez-nous dès aujourd'hui et nous seront ravis d'étudier votre projet.
			</p>
		</div>
		<%= image_tag(asset_path('Ellipse.png'), class: 'floating-M index0') %>
	</div>
</div>

<div class="referencement flex-center flex-column background-img relative" data-aos="zoom-in">
	<div class="flex-start flex-row mt-4 flex-around index1">
		<%= image_tag(asset_path('referencement.png'), alt: 'référencement', class: 'img-referencement mx-5 img-SEO') %>
		<div class="flex flex-column">
			<h2 class="mb-5 h2 text-left ml-5 black">
				Référencement Naturel et Payant<br>
				ou SEO (Search Engine Optimization) / SEA (Advertising)
			</h2>
			<div class="referencement-text flex-center flex-row flex-around" data-aos="zoom-in-left">
				<div class="referencement-column mx-5 text-center">
					<h3 class="h3">Référencement</h3>
					<p class="taupe">Optimiser la position votre site et emmenez-le en première page Google</p>
				</div>
				<div class="referencement-column mx-5 text-center">
					<h3 class="h3">Rédaction web</h3>
					<p class="taupe">L’agence met à votre service sa maitrise des mots-clés et de la sémantique</p>
				</div>
				<div class="referencement-column mx-5 text-center">
					<h3 class="h3">Pénalités Google</h3>
					<p class="taupe">Votre site a subit des penalités Google ? Nous pouvons vous aider</p>
				</div>
			</div>
		</div>
	</div>
	<%= image_tag(asset_path('background-curve.svg'), class: 'absolute t-14 w100 index0 img-curve') %>
</div>

<div class="site flex-center flex-column mb-100 relative" data-aos="zoom-in">
	<h2 class="h2 text-left black index1">Création de votre site internet</h2>
	<p class='mb-100 budget teel index1'>Un besoin, une solution adaptée, un budget maitrisé.</p>
	<div class="flex-row flex-around w100 index1">
		<div class="site-column relative text-center flex-center" data-aos="fade-up-right">
			<h3 class="index1 black-olive font-30">Site vitrine</h3>
			<p class="index1 taupe font-25px">Valorisez votre image avec site internet, cartes de visite et flyers</p>
			<%= image_tag(asset_path('Ellipse-4.png'), class: 'absolute index0 cercle-site') %>
		</div>
		<div class="site-column relative text-center flex-center" data-aos="fade-up">
			<h3 class="index1 black-olive font-30">site E-commerce</h3>
			<p class="index1 taupe font-25px">Créez votre boutique en ligne avec interface administrateur</p>
			<%= image_tag(asset_path('Ellipse-4.png'), class: 'absolute index0 cercle-site') %>
		</div>
		<div class="site-column relative text-center flex-center" data-aos="fade-up-left">
			<h3 class="index1 black-olive font-30">Application web</h3>
			<p class="index1 taupe font-25px">Les meilleurs technologies à votre disposition</p>
			<%= image_tag(asset_path('Ellipse-7.png'), class: 'absolute index0 cercle-site') %>
		</div>
	</div>
	<p class="mt-100 maquettage black-olive index1" data-aos="flip-left">Toute création passe par une phase de maquettage</p>
	<%= image_tag(asset_path('Ellipse-2.svg'), class: 'absolute index0 img-curve-left') %>
</div>

<div class="competences mt-100 mb-100 pb-100 pt-100 flex-center flex-column index1">
	<div class="flex-around flex-row competence-ligne1 index1">
		<div class="graphique relative index0">
			<%= image_tag(asset_path('charte-graphique.svg'), class: 'absolute index-1 charte-graphique t-28') %>
			<p class="khaki text-center index1 w300">charte graphique</p>
		</div>
		<div class="ux relative index0">
			<%= image_tag(asset_path('ux-design.svg'), class: 'absolute index-1 UX-design t-28') %>
			<p class="teel text-center index1 w300">UX design</p>
		</div>
	</div>
	<div class="flex-between flex-row competence-ligne2 index1">
		<div class="création relative index0">
			<%= image_tag(asset_path('creation.svg'), class: 'absolute index-1 charte-graphique t-64 l-0') %>
			<p class="black-olive text-center index1 w300">Création de site internet</p>
		</div>
		<div class="refonte relative index0">
			<%= image_tag(asset_path('refonte.svg'), class: 'absolute index-1 UX-design t-64 r-0') %>
			<p class="blue text-center index1 w300">Refonte site internet</p>
		</div>
	</div>
	<div class="flex-between flex-row nos-competences index1" data-aos="zoom-in-down">
		<div class="création relative index0">
			<%= image_tag(asset_path('Ellipse-8.png'), class: 'absolute index-1 nos-competences-img') %>
			<h2 class="black text-center index1 h2">Nos compétences</h2>
			<h3 class="taupe text-center index1 h3 mt-4">Sont transversales et couvrent tous vos besoins</h3>
		</div>
	</div>
	<div class="flex-between flex-row competence-ligne3 index1">
		<div class="trafic relative index0">
			<%= image_tag(asset_path('trafic.svg'), class: 'absolute index-1 trafic-img t-28') %>
			<p class="teel text-center index1 w300">Acquisition de trafic</p>
		</div>
		<div class="communication relative index0">
			<%= image_tag(asset_path('communication.svg'), class: 'absolute index-1 communication-img t-28') %>
			<p class="black-olive text-center index1 w300">Supports de communication</p>
		</div>
	</div>
	<div class="flex-center flex-row competence-ligne4 index1">
		<div class="refrencement relative index0">
			<%= image_tag(asset_path('payant.svg'), class: 'absolute index-1 refrencement-img t-28 l-23') %>
			<p class="khaki text-center index1 w300">Référencement payant</p>
		</div>
	</div>
</div>

<div class="confiance-section" data-aos="zoom-in">
	<h2 class="h2 black text-center mb-5 rb-20 index1">Ils nous font confiance</h2>
	<%= render "shared/confiance" %>
	<%= image_tag(asset_path('Ellipse-9.svg'), class: 'absolute index0 ellipse-confiance') %>
</div>

<%= render "shared/footer" %>
<%= render 'cookies_eu/consent_banner'%>